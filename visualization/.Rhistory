pima <- read.csv('pima.csv')
# Сделаем более детализированную переменную возрастных групп
pima <- pima %>%
mutate(
diabetes_ch = as.character(diabetes),
age_group = case_when(
age < 31 ~ "21-30",
age >= 31 & age < 41 ~ "31-40",
age >= 41 & age < 51 ~ "41-50",
age >= 51 & age < 61 ~ "51-60",
age >= 61 ~ "60+"
))
table(pima$age_group)
