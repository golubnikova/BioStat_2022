skimr::skim(pima)
plot_ly(
pima[pima$insulin != 0,],
x = ~ insulin,
color = ~ age_group,
type = "box"
)
plot_ly(data = pima[(pima$insulin != 0) & (pima$glucose != 0),],
x = ~ insulin,
y = ~ glucose)
